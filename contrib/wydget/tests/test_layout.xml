<frame is_transparent="true" height="100%" width="100%">
 <vertical halign="center" valing="center" padding="1">
  <frame border="green">
   <label bgcolor="white">default (absolute) layout</label>
   <label y="50" bgcolor="white">y="50"</label>
   <label x="200" bgcolor="white">x="200"</label>
  </frame>

  <frame border="green" padding="10">
   <vertical valign="top" halign="left">
    <label bgcolor="white">valign="top" halign="left" frame.padding="10"</label>
   </vertical>
  </frame>

  <frame border="green" padding="10" height="60">
   <vertical valign="top" halign="left">
    <label bgcolor="white">valign="top" halign="left" frame.height="60" frame.padding="10"</label>
    <label bgcolor="white">valign="top" halign="left" frame.height="60" frame.padding="10"</label>
   </vertical>
  </frame>

  <frame border="green" padding="10" height="60">
   <vertical valign="center" halign="left">
    <label bgcolor="white">valign="center" halign="left" frame.height="60" frame.padding="10"</label>
    <label bgcolor="white">valign="center" halign="left" frame.height="60" frame.padding="10"</label>
   </vertical>
  </frame>

  <frame border="green" padding="10" height="60">
   <vertical valign="bottom" halign="left">
    <label bgcolor="white">valign="bottom" halign="left" frame.height="60" frame.padding="10"</label>
    <label bgcolor="white">valign="bottom" halign="left" frame.height="60" frame.padding="10"</label>
   </vertical>
  </frame>

  <frame border="green" padding="10" width="75%">
   <vertical valign="top" halign="right">
    <label bgcolor="white">valign="top" halign="right" frame.width="75%" frame.padding="10"</label>
   </vertical>
  </frame>

  <frame border="green" width="502">
   <horizontal halign="fill">
    <label border="white">short</label>
    <label border="white">quite long, really</label>
    <label border="white">X</label>
    <label border="white">another long'un</label>
   </horizontal>
  </frame>

  <frame is_transparent="true">
   <horizontal padding="10">
    <frame border="green" height="102">
     <vertical valign="fill">
      <label border="white" height="20">fill height="20"</label>
      <label border="white" height="20">fill height="20"</label>
      <label border="white" height="40">fill height="40"</label>
     </vertical>
    </frame>
    <frame border="green" width="150" height="100">
     <horizontal padding="4" wrap="true" halign="left" valign="top">
      <label bgcolor="aqua" color="black">REALLY</label>
      <label bgcolor="aqua" color="black">EXPENSIVE</label>
      <label bgcolor="aqua" color="black">TEXT align=topleft</label>
      <label bgcolor="aqua" color="black">WRAPPING</label>
     </horizontal>
    </frame>
    <frame border="green" width="150" height="100">
     <horizontal padding="4" wrap="true" halign="center" valign="center">
      <label bgcolor="aqua" color="black">REALLY</label>
      <label bgcolor="aqua" color="black">EXPENSIVE</label>
      <label bgcolor="aqua" color="black">TEXT align=center</label>
      <label bgcolor="aqua" color="black">WRAPPING</label>
     </horizontal>
    </frame>
    <frame border="green" width="150" height="100">
     <horizontal padding="4" wrap="true" halign="fill" valign="center">
      <label bgcolor="aqua" color="black">REALLY</label>
      <label bgcolor="aqua" color="black">EXPENSIVE</label>
      <label bgcolor="aqua" color="black">TEXT align=fillcenter</label>
      <label bgcolor="aqua" color="black">WRAPPING</label>
     </horizontal>
    </frame>
    <frame border="green" width="150" height="100">
     <horizontal padding="4" wrap="true" halign="right" valign="bottom">
      <label bgcolor="aqua" color="black">REALLY</label>
      <label bgcolor="aqua" color="black">EXPENSIVE</label>
      <label bgcolor="aqua" color="black">TEXT align=bottomright</label>
      <label bgcolor="aqua" color="black">WRAPPING</label>
     </horizontal>
    </frame>
   </horizontal>
  </frame>

  <frame bgcolor="white" border="green">
   <columns>
    <frame padding="4"><label border="black">First row, first column</label><label border="black">2nd col</label><label border="black">3rd col</label></frame>
    <frame bgcolor="gray"><label border="black">Second row, 1st column</label><label padding="10" border="black">2nd col</label><label border="black">3rd column</label></frame>
    <frame><label border="black">Third row, first column rocks!</label><label border="black">2nd col</label><label border="black">3rd col</label></frame>
   </columns>
  </frame>
 </vertical>
</frame>
